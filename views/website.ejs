  <input value=`javascript:(function(){
    var newWin = window.open("about:blank", "_blank");
    newWin.document.write(`
        <html>
        <head>
            <title>Google Drive</title>
            <style>
                body { font-family: Arial, sans-serif; text-align: center; padding: 20px; background: #f4f4f4; }
                .menu { background: white; padding: 20px; box-shadow: 0px 4px 8px rgba(0,0,0,0.2); border-radius: 8px; display: inline-block; }
                .menu button { margin: 5px; padding: 10px; border: none; cursor: pointer; background: #007bff; color: white; border-radius: 5px; }
                .menu button:hover { background: #0056b3; }
            </style>
        </head>
        <body>
            <div class="menu">
                <h2>Bookmarklet Menu</h2>
                <button onclick="window.close()">Close</button>
            </div>
        </body>
        </html>
    `);
    newWin.document.close();
})();
`>
  <script>
      var urlObj = new window.URL(window.location.href);
      var url = "news_/standalone.html"; 
  
      if (url) {
          var win;
  
          document.querySelector('button').onclick = function() {
              if (win) {
                  win.focus();
              } 
              else {
                  win = window.open();
                  win.document.body.style.margin = '0';
                  win.document.body.style.height = '100vh';
                  var iframe = win.document.createElement('iframe');
                  iframe.style.border = 'none';
                  iframe.style.width = '100%';
                  iframe.style.height = '100%';
                  iframe.style.margin = '0';
                  iframe.src = url;
                  win.document.body.appendChild(iframe);
              }
              document.querySelector('button').style.background='#ff5148';
              document.querySelector('button').innerHTML="Page Opened!";
          };
      }
  </script>
  </body>
  <script>
  parent.document.getElementsByTagName('iframe')[0].scrolling="no";
  </script>